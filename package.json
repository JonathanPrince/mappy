  "scripts": {
    "test:ci": "npm run jshint && npm run build:assets && mocha -u mocha-given -R spec --compilers js:mocha-traceur test/**/*.spec.js",
    "test:dev": "testem -g",
    "test": "npm run build:assets && npm run test:dev",
    "build:prod": "./build --es6 --minify --brfs ./index.js ./dist/bundle-min.js",
    "build:dev": "./build --es6 --debug --brfs ./index.js ./dist/bundle.js",
    "build:watch": "./build -w -e -d -b -s dist -p 3000 ./index.js ./dist/bundle.js",
    "build:assets": "node ./config/assets/assetBundler.js",
    "build:all": "npm run build:assets && npm run build:dev && npm run build:prod",
    "build": "npm run build:all",
    "jshint": "jshint .",
    "start:dev": "npm run build:assets && npm run build:watch",
    "start": "supervisor -- node_modules/.bin/http-server -p 5000 dist"
